      "type": "set",
      "items": {
        "type": {
          "$ref": "#/definitions/import"
        }
      }
    },
    "jobs": {
      "type": "object",
      "properties": {
        "allow_failures": {
          "type": "array",
          "items": {
            "type": {
              "$ref": "#/definitions/job"
            }
          }
        },
        "exclude": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "stage": {
                "type": "string"
              }
            },
            "$extends": "#/definitions/jobBase",
            "name": "document"
          }
        },
        "fast_finish": {
          "type": "boolean"
        },
        "include": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "stage": {
                "type": "string"
              }
            },
            "$extends": "#/definitions/jobBase",
            "name": "document"
          }
        }
      },
      "name": "document"
    },
