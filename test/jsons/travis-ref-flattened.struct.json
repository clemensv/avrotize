      },
      "name": "document"
    },
    "stages": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "version": {
      "type": "string",
      "pattern": "^(~>|>|>=|=|<=|<) (\\d+(?:\\.\\d+)?(?:\\.\\d+)?)$"
    }
  },
  "$extends": "#/definitions/jobBase",
  "$id": "https://json.schemastore.org/travis.json",
  "$uses": [
    "JSONStructureAlternateNames",
    "JSONStructureConditionalComposition",
    "JSONStructureValidation"
