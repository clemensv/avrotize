    "slackRoom": {
      "type": "string",
      "pattern": ".+:.+(#.+)?",
      "name": "slackRoom"
    },
    "notificationFrequency": {
      "type": "string",
      "enum": [
        "always",
        "never",
        "change"
      ],
      "name": "notificationFrequency"
