      "name": "document"
    },
    "notifications": {
      "type": "object",
      "properties": {
        "campfire": {
          "type": {
            "$ref": "#/definitions/possiblySecretStringOrPossiblySecretStringTypeArrayUnique"
          }
        },
        "email": {
          "type": {
            "$ref": "#/definitions/possiblySecretStringOrPossiblySecretStringTypeArrayUnique"
          }
        },
        "flowdock": {
          "type": {
            "$ref": "#/definitions/possiblySecretString"
          }
        },
        "hipchat": {
          "type": {
            "$ref": "#/definitions/possiblySecretStringOrPossiblySecretStringTypeArrayUnique"
          }
        },
        "irc": {
