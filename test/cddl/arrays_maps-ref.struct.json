{
  "$schema": "https://json-structure.org/meta/extended/v0/#",
  "$id": "https://example/com/schema.json",
  "definitions": {
    "string_list": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "name": "string_list",
      "description": "CDDL schema demonstrating arrays and maps Based on RFC 8610 Section 3.4 Homogeneous arrays",
      "altnames": {
        "cddl": "string-list"
      }
    },
    "int_array": {
      "type": "array",
      "items": {
        "type": "int64"
      },
      "name": "int_array",
      "altnames": {
        "cddl": "int-array"
      }
    },
    "optional_bytes": {
      "type": "array",
      "items": {
        "type": "binary"
      },
      "name": "optional_bytes",
      "altnames": {
        "cddl": "optional-bytes"
      }
    },
    "bounded_list": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "name": "bounded_list",
      "description": "Bounded arrays",
      "altnames": {
        "cddl": "bounded-list"
      }
    },
    "fixed_tuple": {
      "type": "tuple",
      "properties": {
        "_0": {
          "type": "string"
        },
        "_1": {
          "type": "int64"
        },
        "_2": {
          "type": "boolean"
        }
      },
      "tuple": [
        "_0",
        "_1",
        "_2"
      ],
      "name": "fixed_tuple",
      "altnames": {
        "cddl": "fixed-tuple"
      }
    },
    "string_map": {
      "type": "map",
      "keys": {
        "type": "string"
      },
      "values": {
        "type": "any"
      },
      "name": "string_map",
      "description": "Maps with string keys",
      "altnames": {
        "cddl": "string-map"
      }
    },
    "typed_map": {
      "type": "map",
      "keys": {
        "type": "string"
      },
      "values": {
        "type": "int64"
      },
      "name": "typed_map",
      "altnames": {
        "cddl": "typed-map"
      }
    },
    "config_map": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string"
        },
        "port": {
          "type": "int64"
        },
        "timeout": {
          "type": "int64"
        },
        "retries": {
          "type": "int64",
          "default": 3
        }
      },
      "required": [
        "host",
        "port"
      ],
      "name": "config_map",
      "description": "Maps with specific keys",
      "altnames": {
        "cddl": "config-map"
      }
    },
    "nested_array_item_tuple_1": {
      "type": "tuple",
      "properties": {
        "_0": {
          "type": "string"
        },
        "_1": {
          "type": "int64"
        }
      },
      "tuple": [
        "_0",
        "_1"
      ],
      "name": "nested_array_item_tuple_1"
    },
    "nested_array": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/nested_array_item_tuple_1"
      },
      "name": "nested_array",
      "description": "Nested structures",
      "altnames": {
        "cddl": "nested-array"
      }
    },
    "array_of_objects_item_object_2": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "any"
        }
      },
      "required": [
        "name",
        "value"
      ],
      "name": "array_of_objects_item_object_2"
    },
    "array_of_objects": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/array_of_objects_item_object_2"
      },
      "name": "array_of_objects",
      "altnames": {
        "cddl": "array-of-objects"
      }
    }
  },
  "type": "array",
  "items": {
    "type": "string"
  },
  "name": "string_list",
  "description": "CDDL schema demonstrating arrays and maps Based on RFC 8610 Section 3.4 Homogeneous arrays",
  "altnames": {
    "cddl": "string-list"
  },
  "$uses": [
    "JSONStructureAlternateNames",
    "JSONStructureValidation"
  ]
